<h1>Articles</h1>

<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
    <p>
      <%= link_to "Show this article", article %>
    </p>
    <p>
      <%= link_to "Edit Article", edit_article_path(article)%>
    </p>
  <% end %>
</div>

<p>
    <%= link_to "Create new article", new_article_path %>
</p>